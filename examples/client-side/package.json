{
  "name": "react-multistep-example",
  "homepage": "https://srdjan.github.io/react-multistep",
  "version": "6.0.0",
  "license": "MIT",
  "scripts": {
    "debug": "esbuild app.js --bundle --format=cjs --outfile=./bundle.js --loader:.js=jsx --sourcemap --preserve-symlinks",
    "build:comp": "cd ../.. && tsc && cd ./examples/client-side",
    "inst:comp": "mkdir -p node_modules/react-multistep && cp -Rf ../../build/** node_modules/react-multistep/",
    "build:app": "esbuild app.js --bundle --format=cjs --outfile=./bundle.js --loader:.js=jsx --minify --define:process.env.NODE_ENV='\"production\"'",
    "build": "npm run build:comp && npm run inst:comp && cp ../../src/base.css ./css/multistep.css && npm run build:app",
    "start": "npm run build:app && npx http-server ./ -p 3000"
  },
  "alias": {
    "react": "./node_modules/react"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.10"
  }
}
